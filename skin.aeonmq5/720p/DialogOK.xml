			<width>580</width>
			<height>35</height>
			<align>center</align>
			<textcolor>FFFFFAF0</textcolor>
			<font>Font_17</font>
			<scroll>true</scroll>
		</control>
		<control type="label" id="4">
			<posx>360</posx>
			<posy>285</posy>
			<width>580</width>
			<height>35</height>
			<align>center</align>
			<textcolor>FFFFFAF0</textcolor>
			<font>Font_17</font>
			<scroll>true</scroll>
		</control>
		<control type="button" id="10">
			<posx>565</posx>
			<posy>340</posy>
			<height>29</height>
			<width>150</width>
			<align>center</align>
			<textcolor>FFFFFAF0</textcolor>
			<focusedcolor>FF101010</focusedcolor>
			<font>Font_18A</font>
			<label>$LOCALIZE[186]</label>
			<textoffsetx>0</textoffsetx>
			<textwidth>180</textwidth>
			<texturefocus>settings_button_focus.png</texturefocus>
